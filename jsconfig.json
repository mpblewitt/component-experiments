{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "~/*": [
          "./src/*"
        ],
        "@/*": [
          "./src/*"
        ],
        "~~/*": [
          "./*"
        ],
        "@@/*": [
          "./*"
        ]
      }
    },
    "include": [
      "./**/*"
    ],
    "exclude": [
      "node_modules",
      ".nuxt",
      "dist"
    ]
  }